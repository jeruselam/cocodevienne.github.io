---
sitemap: false
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="https://www.w3.org/2005/Atom">

 <title>Zikr Tercume</title>
 <link href="http://jeruselam.github.io/atom.xml" rel="self"/>
 <link href="http://jeruselam.github.io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://jeruselam.github.io/</id>
 <author>
   <name>Zikr</name>
   <email>tercume@linux.org</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="https://vdemir.github.io{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>https://jeruselam.github.io{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
